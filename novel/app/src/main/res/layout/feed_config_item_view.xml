<?xml version="1.0" encoding="utf-8"?>
<com.duoyue.lib.base.widget.XLinearLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        android:gravity="center_horizontal">

    <com.duoyue.lib.base.widget.XLinearLayout android:layout_width="match_parent"
                                              android:layout_height="match_parent">
        <TextView
                android:id="@+id/tv_config"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:x_layout_padding="#,26,#,26"
                style="@style/QuestionErrorRadioButton"
                android:lines="1"
                android:ellipsize="end"
                android:background="@drawable/question_error_bg_uncheck"/>
    </com.duoyue.lib.base.widget.XLinearLayout>

</com.duoyue.lib.base.widget.XLinearLayout>
